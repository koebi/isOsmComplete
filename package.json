{
  "name": "is-osm-complete",
  "version": "0.1.0",
  "description": "A website for checking completeness of a osm",
  "author": "",
  "license": "MIT",
  "homepage": "https://wvanderp.github.io/isOsmComplete/",
  "repository": {
    "type": "git",
    "url": "https://github.com/wvanderp/isOsmComplete"
  },
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "staticFiles": {
    "staticPath": "data"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.10.0",
    "@parcel/transformer-sass": "^2.10.0",
    "@types/crypto-js": "^4.1.2",
    "@types/d3": "^7.4.1",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/unzipper": "^0.10.7",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-nibble": "^8.1.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "parcel": "2.10.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "process": "^0.11.10",
    "rimraf": "^5.0.5",
    "sass": "^1.69.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "@types/mocha": "^10.0.2",
    "@types/chai": "^4.3.8",
    "mocha": "^10.2.0",
    "chai": "^4.3.10"
  },
  "dependencies": {
    "@rapideditor/country-coder": "^5.2.2",
    "axios": "^1.5.1",
    "bootstrap": "^5.3.2",
    "country-code-emoji": "^2.3.0",
    "country-flag-emoji-polyfill": "^0.1.4",
    "crypto-js": "^4.1.1",
    "csv-parse": "^5.5.2",
    "d3": "^7.8.5",
    "ajv": "^8.12.0",
    "expect": "^29.7.0",
    "i18n-iso-countries": "^7.7.0",
    "overpass-ts": "^4.3.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactstrap": "^9.2.0",
    "unzipper": "^0.10.14",
    "url-join": "^5.0.0"
  },
  "scripts": {
    "app": "npm run clean && parcel serve app/index.html -p 3001",
    "build": "npm run clean &&  parcel build --no-source-maps --public-url /isOsmComplete --dist-dir ./gitBuild app/index.html && ts-node ./scripts/postProcess.ts",
    "lint": "npm run tsc && eslint --ext ts,tsx,js app/ collect/",
    "lintData": "ts-node -T collect/lint/lint.ts",
    "tsc": "tsc --noEmit",
    "collect": "ts-node collect && npm run lintData",
    "clean": "rimraf .parcel-cache dist gitBuild",
    "test": "mocha --recursive tests/**/*.spec.ts tests/**/**/*.spec.ts"
  }
}
